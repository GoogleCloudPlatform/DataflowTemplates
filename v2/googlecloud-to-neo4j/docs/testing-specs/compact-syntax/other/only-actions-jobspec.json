{
  "config": {
    "debug": true,
    "reset_db": true,
    "index_all_properties": true,
    "node_write_batch_size": 5000,
    "edge_write_batch_size": 1000,
    "node_write_parallelism": 10,
    "edge_write_parallelism": 1
  },
  "actions": [
    {
      "name": "Pre Action cypher, node count",
      "execute_after": "start",
      "type": "cypher",
      "options": [
        {"cypher": "MATCH (n)\nRETURN count(n)"}
      ]
    },
    {
      "name": "Post Action cypher, node count",
      "execute_after": "loads",
      "type": "cypher",
      "options": [
        {"cypher": "MATCH (n)\nRETURN count(n)"}
      ]
    },
    {
      "name": "Pre Action GET test url",
      "execute_after": "start",
      "type": "http_get",
      "options": [
        {"url": "https://httpbin.org/get"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    },
    {
      "name": "Post Action GET test url",
      "execute_after": "loads",
      "type": "http_get",
      "options": [
        {"url": "foo"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    },
    {
      "name": "Pre Action POST test url",
      "execute_after": "start",
      "type": "http_post",
      "options": [
        {"url": "https://httpbin.org/post"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    },
    {
      "name": "Post Action POST test url",
      "execute_after": "loads",
      "type": "http_post",
      "options": [
        {"url": "https://httpbin.org/post"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    },
    {
      "name": "Pre Action BigQuery",
      "execute_after": "start",
      "type": "bigquery",
      "options": [
        {"sql": "SELECT count(*) FROM neo4j-se-team-201905.northwind.V_CUSTOMER_ORDERS"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    },
    {
      "name": "Post Action BigQuery",
      "execute_after": "loads",
      "type": "bigquery",
      "options": [
        {"sql": "SELECT count(*) FROM neo4j-se-team-201905.northwind.V_CUSTOMER_ORDERS"}
      ],
      "headers": [
        {"header1": "value1"},
        {"header2": "value2"}
      ]
    }
  ]
}