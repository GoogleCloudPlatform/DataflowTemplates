<configuration>
  <property>
    <name>hbase.zookeeper.property.initLimit</name>
    <value>20</value>
  </property>
  <property>
    <name>hbase.rootdir</name>
    <value>hdfs://shitanshu-test-m/hbase</value>
  </property>
  <property>
    <name>hbase.zookeeper.quorum</name>
    <value>shitanshu-test-m:2181</value>
  </property>
  <property>
    <name>hbase.cluster.distributed</name>
    <value>true</value>
  </property>
  <property>
    <name>google.bigtable.project.id</name>
    <value>google.com:cloud-bigtable-dev</value>
  </property>
  <property>
    <name>google.bigtable.instance.id</name>
    <value>shitanshu-test</value>
  </property>
  <property>
    <name>google.bigtable.table</name>
    <value>test-table</value>
  </property>
</configuration>